{
  "name": "oxycore/framework",
  "description": "The Oxycore Framework",
  "type": "library",
  "license": "MIT",
  "keywords": ["framework", "oxycore", "oxylion"],
  "homepage": "https://github.com/Oxycore/Framework",
  "support": {
    "issues": "https://github.com/Oxycore/Framework/issues",
    "source": "https://github.com/Oxycore/Framework"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.9.x-dev"
    }
  },
  "require": {
    "php": ">=7.4||^8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-xml": "*",
    "ext-yaml": "*",
    "psr/container": "^1.0",
    "vlucas/phpdotenv": "^5.2"
  },
  "require-dev": {
    "filp/whoops": "^2.4",
    "mockery/mockery": "^1.3.1",
    "phpunit/phpunit": "^8.4|^9.0",
    "twig/twig": "^1.6 || ^2.0 || ^3.0 || ^4.0 || ^5.0",
    "symfony/yaml": "^2.3 || ^3.0 || ^4.0 || ^5.0"
  },
  "autoload": {
    "files": [
      "src/oxycore/Framework/helpers.php",
      "src/oxycore/Foundation/helpers.php",
      "src/oxycore/Support/helpers.php"
    ],
    "psr-4": {
      "Oxycore\\": "src/oxycore/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oxycore\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "ext-ftp": "Required to use the Flysystem FTP driver.",
    "ext-gd": "Required to use Oxycore\\Foundatiom\\Http\\Testing\\FileFactory::image().",
    "ext-memcached": "Required to use the memcache cache driver.",
    "ext-pcntl": "Required to use all features of the queue worker.",
    "ext-posix": "Required to use all features of the queue worker.",
    "ext-redis": "Required to use the Redis cache and queue drivers (^4.0|^5.0)."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
